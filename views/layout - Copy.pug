doctype html
html(lang="en")
	head
		meta(charset='utf-8')
		meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
		meta(name='description', content='')
		meta(name='author', content='')
		link(rel='icon', href='/images/favicon.ico')
		title #{title} | DELL
		block styles
			link(href='/stylesheets/bootstrap.min.css', rel='stylesheet')
			link(href='/stylesheets/bootstrap-grid.min.css', rel='stylesheet')

	body
		header
			nav.navbar.navbar-expand-md.navbar-dark.fixed-top.bg-dark
				a.navbar-brand(href="#") DELL
				button.navbar-toggler(type="button", data-toggle="collapse", data-target="#navbarCollapse", aria-controls="navbarCollapse", aria-expanded="false", aria-label="Toggle navigation")
					span.navbar-toggler-icon
				div.collapse.navbar-collapse#navbarCollapse
					ul.navbar-nav.mr-auto
						li.nav-item.active
							a.nav-link(href="#") Intro
								span.sr-only (current)
						li.nav-item
							a.nav-link(href="#") Inventory
						li.nav-item
							a.nav-link Analyze
						li.nav-item
							a.nav-link About

		hr.hr-primary
		main(role="main")
			block content

			footer.container.fixed-bottom
				p.float-right
					a(href="#") Back to top

		script(src='https://code.jquery.com/jquery-3.3.1.slim.min.js')
		script(src='/javascripts/bootstrap.bundle.min.js')